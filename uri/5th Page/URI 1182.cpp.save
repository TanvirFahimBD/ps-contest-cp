#include<iostream>
#include<stdio.h>
#include<i
using namespace std;

int main()
{
    int cn, r, c, arr[12][12];
    char ch[2];
    double sum = 0.0, avg = 0.0;
    cin >> cn;
    scanf("%s", &ch);
    for(r = 0; r < 2; r++){
        for(c = 0; c < 2; c++){
            cin >> arr[r][c];
            if(c == cn){
                sum += arr[r][c];
            }
        }
    }
    avg = sum / 2.0;
    if(ch[0] == 'S') cout << fixed << setprecision(1) << sum << endl;
    else if(ch[0] == 'M') cout << fixed << setprecision(1) << avg << endl;

    return 0;
}


