#include<iostream>
using namespace std;

int main()
{
    int n, sm, j;
    cin >> n;
    int s[n+2];
    for(int i = 0; i<n; i++){
        cin >> s[i];
    }
    sm = s[0];
    for(int i = 0; i<n; i++){
        if(s[i] < s[i-1]){
            sm = s[i];
            j = i;
        }else continue;
    }
    
    cout << ""

    return 0;
}
