#include<iostream>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while(1){
        int x, sum = 0;
        cin >> x;
        for(int i = 0; i<=x; i++){
            if(x%i == 0){
                sum += i;
            }
        }
        if(sum == x){
            cout << x << " eh perfeito" << endl;
        }else cout << x << " nao eh perfeito" << endl;
        t--;
    }

    return 0;
}
