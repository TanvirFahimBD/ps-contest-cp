#include<iostream>
using namespace std;

int main()
{
    while(1){
        int n, i = 0, sum = 0;
        cin >> n;
        if(n%2 == 0){
            for(; i<5; i++){
                sum += n;
                n += 2;
            }
        }else if(n%2 != 0){
            n = n+1;
            for(; i<5; i++){
                sum += n;
                n += 2;
            }
        cout << sum << endl;
        }else if(n == 0){
            cout << 0 << endl;
            break;
        }

    }

    return 0;
}

